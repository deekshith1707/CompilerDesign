#include <stdio.h>

int add(int a, int b) {
    return a + b;
}

int sub(int a, int b) {
    return a - b;
}

int multiply(int a, int b) {
    return a * b;
}

int main() {
    int res;
    
    printf("--- Function Pointer Test ---\n\n");
    
    // Test 1: Direct function call (ADD)
    printf("Test 1: Direct Function Call (ADD)\n");
    res = add(10, 5);
    printf("Result of add(10, 5): %d (Expected: 15)\n\n", res);
    
    // Test 2: Direct function call (SUB)
    printf("Test 2: Direct Function Call (SUB)\n");
    res = sub(20, 7);
    printf("Result of sub(20, 7): %d (Expected: 13)\n\n", res);
    
    // Test 3: Direct function call (MULTIPLY)
    printf("Test 3: Direct Function Call (MULTIPLY)\n");
    res = multiply(6, 7);
    printf("Result of multiply(6, 7): %d (Expected: 42)\n\n", res);
    
    // Test 4: Nested function calls
    printf("Test 4: Nested Function Calls\n");
    res = add(multiply(5, 2), sub(20, 5));
    printf("Result of add(multiply(5, 2), sub(20, 5)): %d (Expected: 25)\n\n", res);

    
    return 0;
}