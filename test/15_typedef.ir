    buf[3] = 'r'
    buf[4] = 't'
    buf[5] = '\0'
    curr = ON
    t1 = &dev
    *(t1 + 0) = pID
    t2 = &data
    *(t2 + 0) = 3.14f
    t3 = &dev
    t4 = *(t3 + 0)
    t5 = *t4
    t6 = &data
    t7 = *(t6 + 0)
    param "ID: %d, Mode: %d, Buffer: %s, Data: %.1f\n"
